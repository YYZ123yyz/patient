<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="114.5dp"
    android:background="@color/color_f6f6f6"
    android:orientation="vertical">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="解约"
        android:layout_marginStart="22.5dp"
        android:layout_centerVertical="true"
        android:textColor="@color/white"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:paddingStart="35dp"
        android:paddingEnd="35dp"
        android:id="@+id/tv_cancel_contract_btn"
        android:background="@drawable/bg_round_7a9dd8_15"
        />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="取消"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="22.5dp"
        android:textColor="@color/white"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:paddingStart="35dp"
        android:paddingEnd="35dp"
        android:id="@+id/tv_cancel_btn"
        android:background="@drawable/bg_round_7a9dd8_15"
        />
</RelativeLayout>