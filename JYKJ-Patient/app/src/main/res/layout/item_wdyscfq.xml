<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="15dp"
    android:background="@color/writeColor"
    android:id="@+id/clickLayout"
    android:orientation="vertical"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="10dp"
        android:background="@drawable/bd_wdys_jzlb">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="未设置"
            android:id="@+id/tv_ypmc"
            android:gravity="center_vertical"
            android:textColor="#ff000000"
            android:layout_weight="1"
            android:textSize="16sp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" 未设置"
            android:layout_marginRight="10dp"
            android:id="@+id/tv_cflx"
            android:gravity="center_vertical"
            android:textColor="#ff000000"
            android:textSize="15sp"
            />
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text="未设置"
        android:id="@+id/tv_gmsl"
        android:layout_marginLeft="10dp"
        android:gravity="center_vertical"
        android:textColor="#ff666666"
        android:textSize="15sp"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:text="未设置"
            android:layout_weight="1"
            android:id="@+id/tv_yysl"
            android:gravity="center_vertical"
            android:textColor="#ff666666"
            android:textSize="15sp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:text="未设置"
            android:id="@+id/tv_fypl"
            android:gravity="center_vertical"
            android:textColor="#ff666666"
            android:textSize="15sp"
            />
    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:gravity="center_vertical"
        android:layout_marginLeft="10dp"
        android:id="@+id/tv_fyzq"
        android:text="未设置"
        android:textColor="#ff666666"
        android:textSize="15sp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:gravity="center_vertical"
        android:id="@+id/tv_sm"
        android:layout_marginLeft="10dp"
        android:text="未设置"
        android:textColor="#ff666666"
        android:textSize="15sp"
        />
</LinearLayout>