<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@color/pBackColor"
    android:id="@+id/li_clickLayout"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="1dp"
        android:background="@color/writeColor"
        android:orientation="horizontal">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="2019年11月20日"
            android:textSize="8pt"
            android:layout_marginLeft="10dp"
            android:textColor="@color/tabColor_nomal"
            android:id="@+id/tv_activityPatientLaber_createDate"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="高血压I期"
            android:id="@+id/tv_activityPatientLaber_laberName"
            android:textSize="8pt"
            android:layout_marginRight="10dp"
            android:textColor="@color/tabColor_nomal"
            android:layout_gravity="center_vertical"
            />
    </LinearLayout>

</LinearLayout>